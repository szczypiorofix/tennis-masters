{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "set PORT=8080 && react-scripts start",
        "build:prod:win": "set \"REACT_APP_ENVIRONMENT=production\" && set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
        "build:prod:linux": "REACT_APP_ENVIRONMENT=production GENERATE_SOURCEMAP=false react-scripts build",
        "build:dev:win": "set \"REACT_APP_ENVIRONMENT=development\" && react-scripts build",
        "build:dev:linux": "REACT_APP_ENVIRONMENT=development react-scripts build",
        "build:local:win": "set \"REACT_APP_ENVIRONMENT=localhost\" && react-scripts build",
        "build:local:linux": "REACT_APP_ENVIRONMENT=localhost react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "eslint": "npx  eslint ."
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "styled-components": "^5"
    },
    "dependencies": {
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-router-dom": "^6.28.0",
        "react-scripts": "5.0.0",
        "styled-components": "^6.1.13",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "12.1.5",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^13.5.0",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^18.11.9",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.3.3",
        "@types/styled-components": "^5.1.34",
        "@types/uuid": "^9.0.0",
        "eslint": "^8.57.1",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.6.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "ts-node-dev": "^2.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.9.5",
        "typescript-eslint": "^8.13.0"
    }
}
